#!/usr/bin/env python
#
# kill-done-jobs
#


import sys


def arg_parser():
    from argparse import ArgumentParser
    parser = ArgumentParser()
    parser.add_argument('masterjob',
                        nargs='*',
                        help='Only scan these masterjobs')
    return parser


def main(argv=None):
    if argv is None:
        argv = sys.argv[1:]
    args = arg_parser().parse_args(argv)

    from alimaster import env

    print('ok')


if __name__ == "__main__":
    exit(main())

